{
    "name": "example_task",
    "desc": "An example task that runs every minute",
    "schedule": "* * * * *",
    "enabled": true,
    "modules": [
        {
            "shell": {
                "command": "echo 'Shell task executed'"
            }
        },
        {
            "file": {
                "file": "example.txt",
                "on-change": "echo 'File changed!'"
            }
        }
    ],
    "output": "task_log.txt"
}
